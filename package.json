    "moment": "^2.24.0",
    "nuxt": "^2.0.0",
    "nuxt-property-decorator": "^2.5.1",
    "raw-loader": "^4.0.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/tailwindcss": "^1.0.0",
    "@types/jest": "^25.2.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "prettier": "^2.0.3",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "ts-jest": ">=24.0.0 <25.0.0",
    "vue-jest": "^4.0.0-0"
  }
