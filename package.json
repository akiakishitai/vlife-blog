{
  "name": "vlife-blog",
  "version": "1.0.0",
  "description": "Generate a static site with Nuxt.js",
  "author": "akiakiS",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "analyze": "yarn build --analyze && yarn run webpack-bundle-analyzer .nuxt/stats/client.json .nuxt/dist/client --port 8888 --report .nuxt/stats/client.html --no-open",
    "build": "nuxt build",
    "deploy": "yarn license-file && env DEPLOY_ENV=GH_PAGES NODE_ENV=production nuxt generate",
    "dev": "nuxt",
    "fmt": "yarn prettier --config .prettierrc.yml --write '**/*.{js,ts,vue,json}'",
    "generate": "NODE_ENV=production nuxt generate",
    "license-file": "mkdir -p src/static/txt && yarn licenses generate-disclaimer --ignore-platform > src/static/txt/thirdparty_license.txt",
    "sass-build": "sass -I ./node_modules src/assets/css/scss/",
    "start": "nuxt start",
    "test": "jest --runInBand",
    "test-ci": "jest --runInBand --coverage false",
    "update-articles": "git submodule update --remote",
    "update-caniuse": "yarn run browserslist --update-db"
  },
  "dependencies": {
    "@asciidoctor/core": "^2.2.6",
    "@nuxt/http": "^0.6.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@types/js-beautify": "^1.13.3",
    "asciidoctor-kroki": "^0.16.0",
    "dayjs": "^1.9.1",
    "fast-deep-equal": "^3.1.3",
    "js-beautify": "^1.14.7",
    "material-colors": "^1.2.6",
    "material-components-web": "^7.0.0",
    "nunjucks": "^3.2.2",
    "nuxt": "^3.4.3",
    "nuxt-property-decorator": "^2.5.1",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.27.0",
    "simple-icons": "^3.0.1",
    "vee-validate": "^3.4.3"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/tailwindcss": "^4.0.0",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^26.0.0",
    "@types/jsdom": "^16.2.5",
    "@types/nunjucks": "^3.1.3",
    "@types/prismjs": "^1.16.1",
    "@types/simple-icons": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vue/test-utils": "^1.0.0",
    "babel-jest": "^26.0.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.0.0",
    "patch-package": "^6.4.7",
    "postcss": "^8.1.10",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-url": "^10.1.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.0.3",
    "sass": "^1.26.5",
    "sass-loader": "^10.0.0",
    "ts-jest": "^26.0.0",
    "vue-jest": "^4.0.0-0"
  },
  "peerDependencies": {
    "@babel/core": "^7.11.6",
    "ini": "^1.3.8",
    "typescript": "^4.0.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.5.1",
    "webpack": "^4.44.2 || ^5.0.0"
  },
  "resolutions": {
    "is-svg": "^4.2.2",
    "ssri": "^8.0.1"
  }
}
