{
  "name": "vlife-blog",
  "version": "1.0.0",
  "description": "Generate a static site with Nuxt.js",
  "author": "akiakiS",
  "private": true,
  "scripts": {
    "build": "nuxt-ts build",
    "deploy": "yarn license-file && yarn mdlist && env DEPLOY_ENV=GH_PAGES NODE_ENV=production nuxt-ts generate",
    "dev": "nuxt-ts",
    "fmt": "yarn prettier --config .prettierrc.yml --write '**/*.{js,ts,vue,json}'",
    "generate": "NODE_ENV=production nuxt-ts generate",
    "license-file": "yarn licenses generate-disclaimer --ignore-platform > src/assets/thirdparty_license.txt",
    "mdlist": "node --experimental-modules fileListToJson.mjs",
    "sass-build": "sass -I ./node_modules src/assets/css/scss/",
    "start": "nuxt-ts start",
    "tailwind-build": "tailwindcss build src/assets/css/post/_post-body.css -c src/tailwind.config.js -o src/static/stylesheets/asciidoc-style.css",
    "test": "jest",
    "test-ci": "jest --coverage false"
  },
  "dependencies": {
    "@asciidoctor/core": "^2.2.0",
    "@nuxt/typescript-runtime": "^0.4.0",
    "@types/markdown-it": "^10.0.0",
    "@types/prismjs": "^1.16.1",
    "fast-deep-equal": "^3.1.3",
    "front-matter": "^3.1.0",
    "markdown-it": "^10.0.0",
    "markdown-it-attrs": "^3.0.3",
    "material-colors": "^1.2.6",
    "material-components-web": "^6.0.0",
    "moment": "^2.24.0",
    "nuxt": "^2.0.0",
    "nuxt-property-decorator": "^2.5.1",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.21.0",
    "raw-loader": "^4.0.0",
    "simple-icons": "^3.0.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/tailwindcss": "^2.0.0",
    "@types/jest": "^25.2.1",
    "@types/simple-icons": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^24.1.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^24.1.0",
    "prettier": "^2.0.3",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "ts-jest": ">=24.0.0 <25.0.0",
    "vue-jest": "^4.0.0-0"
  }
}
